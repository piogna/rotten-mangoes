<h1>Admin::Users#index</h1>
<%= link_to "Create New User", new_admin_user_path %>
<%=  paginate @users %>
<table>
  <th>Email</th>
  <th>First</th>
  <th>Last</th>
  <th>Is admin?</th>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.email %></td>
      <td><%= user.firstname %></td>
      <td><%= user.lastname %></td>
      <td><%= user.admin? %></td>
      <td><%= link_to "Edit", edit_admin_user_path(user) %>,
      <%= link_to "Delete", admin_user_path(user),
      :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>